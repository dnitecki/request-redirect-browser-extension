{
  "manifest_version": 3,
  "name": "Request Redirect",
  "version": "0.1.0",
  "description": "Redirect Requests with this extension",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Open",
    "default_icon": {
      "128": "rr-icon-128.png"
    }
  },
  "permissions": ["storage", "scripting"],
  "icons": {
    "32": "rr-icon-32.png",
    "64": "rr-icon-64.png",
    "128": "rr-icon-128.png"
  }
}
